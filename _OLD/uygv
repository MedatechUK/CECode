INSERT INTO GENERALLOAD (LINE, RECORDTYPE, REAL1, TEXT3, INT1)
SELECT :ZLINE + 1 , '2', :ZCLA_QPRICE, ACCOUNTS.ACCNAME,
:ZCLA_PROJACT
FROM   PROJACTS, DOCUMENTS, CUSTOMERS, ACCOUNTS
