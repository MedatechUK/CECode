UPDTYPE (CHAR,1,'I/U/D')
.    I(nsert)
.    U(pdate)
.    D(elete)
EDITTYPE (CHAR,1,'W/A/R/S/H/C/P')
.    W(ays)
.    A(ttachment)
.    R(oom)
.    S(ubcontract)
.    H(ouse)
.    C(onsumer unit)
.    P(art)
*/
#INCLUDE func/ZCLA_DEBUGUSR
SELECT 'ZCLA_HTEDIT/ZCLA_BUF1'
, :$.HOUSETYPE  , :$.EDITTYPE , :UPDTYPE
FROM DUMMY
WHERE :DEBUG = 1
FORMAT ADDTO :DEBUGFILE
;
:HOUSETYPE = 0 ;
SELECT :$.HOUSETYPE INTO :HOUSETYPE
FROM DUMMY ;
