:EXPO ='C:\TMP\EXPO.TXT';
:EXPO1 = '../../SYSTEM/TMP/EXPO1.TXT';
:EXPO2 = '../../SYSTEM/TMP/EXPO2.TXT';
SELECT SQL.TMPFILE INTO :RESPONCE FROM DUMMY;
SELECT SQL.TMPFILE INTO :WSMSG FROM DUMMY;
:NOTABSTITLE = 1;
:EMAIL = '';
