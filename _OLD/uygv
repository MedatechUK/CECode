UPDATE ZCLA_PLOTCOMPONENT
SET     PART = :NEWKEY
WHERE  0=0
AND    GUID = :GUID
/*GergoM | 24/11/2023 |
PROJACT = :ELEMENT, because it updated the component under all plots
*/
AND PROJACT = :ELEMENT
AND    ROOM IN ( SELECT ROOM
FROM ZCLA_PLOTROOMS
WHERE PROJACT =  :ELEMENT
);
