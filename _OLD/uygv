,   FIXID
,   SPLIT
,   VALUE
FROM ZCLA_HTEDITSPLIT
WHERE EDITID = :HTEDIT
;
/*
Run calculation */
#INCLUDE ZCLA_ELACT/RECALC
/*
Set Close Price after Recalc */
:CLOSE_TOTCOST = :CLOSE_TOTPRICE = 0.0 ;
SELECT ZCLA_TOTCOST
,      ZCLA_TOTPRICE
INTO   :CLOSE_TOTCOST
,      :CLOSE_TOTPRICE
FROM PROJACTS
WHERE 0=0
AND   PROJACTS.PROJACT = :ELEMENT
;
/*
Close PLOT/ELEMENT edit */
UPDATE ZCLA_ELEDIT
SET CLOSEFLAG = 'Y'
,   CLOSEDATE = SQL.DATE
,   CLOSE_TOTCOST = :CLOSE_TOTCOST
,   CLOSE_TOTPRICE = :CLOSE_TOTPRICE
WHERE 0=0
AND EDITID = :OPENEDIT
;
