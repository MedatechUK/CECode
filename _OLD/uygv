:INSERT ='W' ; */
SUB 5112311 ;
GOTO 6112310 WHERE :WASDELETED = 'Y' ;
:CU = 0 ;
SELECT CONSUMERUNIT INTO :CU
FROM   ZCLA_PLOTCU
WHERE  PROJACT  = :ELEMENT
AND    GUID     = :PARENT
;
INSERT INTO ZCLA_PLOTCUCFG ( CONSUMERUNIT , GUID , KLINE , PART ,
DEVICE , WAYNO , RCD )
SELECT :CU , :GUID , :KEY1 , :INT1 , :INT2 , :INT3 , :TEXT1
FROM DUMMY
;
GOTO 6112315 ;
LABEL 6112310 ; /* Was Deleted */
UPDATE ZCLA_PLOTCUCFG
SET    PART         = :INT1
,      DEVICE       = :INT2
,      WAYNO        = :INT3
,      RCD          = :TEXT1
,      ISDELETED    = 'N'
WHERE  0=0
AND    GUID = :GUID
AND    CONSUMERUNIT IN (
SELECT CONSUMERUNIT
FROM   ZCLA_PLOTCU
WHERE  PROJACT = :ELEMENT
) ;
LABEL 6112315 ;
RETURN ;
